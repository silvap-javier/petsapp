{
  "name": "petsapp",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prisma:generate": "pnpm --filter api prisma generate",
    "prisma:migrate": "pnpm --filter api prisma migrate dev"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.1",
    "turbo": "^1.12.5",
    "typescript": "^5.4.3"
  },
  "packageManager": "pnpm@8.15.4"
}
